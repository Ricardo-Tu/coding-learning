{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/bash",
      "args": [
        "${workspaceRoot}/vulkan/toy2d-cpp/compiler.sh",
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },
    {
      "type": "process",
      "command": "/usr/bin/env",
      "args": [
        "CC=clang",
        "CFLAGS=-g -Wall",
        "make",
        "${workspaceRoot}/vulkan/toy2d-cpp/build/sandbox/sandbox"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "makelldb: selected file"
    }
  ],
  "version": "2.0.0"
}